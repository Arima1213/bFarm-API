        "/api/users/register/individual": {
            "post": {
                "description": "Registers a new individual user",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "first_name": {
                                        "type": "string"
                                    },
                                    "last_name": {
                                        "type": "string"
                                    },
                                    "phone": {
                                        "type": "string"
                                    },
                                    "user_type": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email"
                                    },
                                    "password": {
                                        "type": "string"
                                    },
                                    "full_address": {
                                        "type": "string"
                                    },
                                    "village_id": {
                                        "type": "integer"
                                    },
                                    "sub_district_id": {
                                        "type": "integer"
                                    },
                                    "city_district_id": {
                                        "type": "integer"
                                    },
                                    "province_id": {
                                        "type": "integer"
                                    },
                                    "postal_code": {
                                        "type": "integer"
                                    },
                                    "image_file": {
                                        "type": "string"
                                    }
                                },
                                "required": [
                                    "first_name",
                                    "last_name",
                                    "phone",
                                    "user_type",
                                    "email",
                                    "password",
                                    "full_address",
                                    "village_id",
                                    "sub_district_id",
                                    "city_district_id",
                                    "province_id",
                                    "postal_code",
                                    "image_file"
                                ]
                            },
                            "examples": {
                                "example1": {
                                    "summary": "Example individual user registration",
                                    "value": {
                                        "first_name": "Ari",
                                        "last_name": "Hens",
                                        "phone": "+1234567890",
                                        "user_type": "individual",
                                        "email": "ari.hens@example.com",
                                        "password": "Password123",
                                        "full_address": "1234 Main St, Anytown",
                                        "village_id": 1234,
                                        "sub_district_id": 5678,
                                        "city_district_id": 91011,
                                        "province_id": 1213,
                                        "postal_code": 12345,
                                        "image_file": "user_image1.jpg"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Individual user successfully registered",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "id": {
                                            "type": "string"
                                        },
                                        "email": {
                                            "type": "string",
                                            "format": "email"
                                        }
                                    }
                                },
                                "example": {
                                    "id": "123456",
                                    "email": "ari.hens@example.com"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request - Missing or invalid input",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "error": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "example": {
                                    "error": "Missing required field: email"
                                }
                            }
                        }
                    },
                    "409": {
                        "description": "Conflict - Email already in use",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
